{
  "name": "battle-rap-api",
  "version": "0.1.0",
  "description": "Battle rap tournament MVP API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "dotenv -e .env -- ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "lint": "eslint \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "test": "jest",
    "migrate": "ts-node --project tsconfig.json scripts/migrate.ts"
  },
  "keywords": [
    "battle-rap",
    "express",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.13.0",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
